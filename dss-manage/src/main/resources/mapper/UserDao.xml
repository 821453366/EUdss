<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eu.dss.dao.UserDao">
    <!--所有用户信息-->
    <select id="findUser" parameterType="PageBean"  resultType="User">
        SELECT * FROM dssuser limit #{index},#{pageCount}
    </select>
    <!--所有用户信息-->
    <select id="getTotalCount"   resultType="int">
        SELECT count(*) FROM dssuser
    </select>

</mapper>