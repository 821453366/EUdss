<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eu.dss.dao.TronClassDao">

    <select id="tronClass"  resultType="TronClass">
        SELECT * FROM eu_tronclass
    </select>

    <select id="tronClassPaging" parameterType="PageBean"  resultType="TronClass">
        SELECT * FROM eu_tronclass limit #{index},#{PageCount}
    </select>


</mapper>